{
  "plugins": ["node_modules/jsdoc-typeof-plugin", "plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "include": [ "src" ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "docs",
    "readme": "README.md"
  }
}
